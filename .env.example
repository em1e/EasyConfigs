# Environment
NODE_ENV=development

# Database
DATABASE_URL=file:./dev.db

# NextAuth Configuration
NEXTAUTH_SECRET=your-super-secret-key-change-this-in-production-please-make-it-long
NEXTAUTH_URL=http://localhost:4000

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:4000

# Email Configuration (Resend)
RESEND_API_KEY=your-resend-api-key-here
FROM_EMAIL=onboarding@resend.dev

# Development Settings
DEV_AUTO_VERIFY_EMAIL=true

# Google OAuth (Get these from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# GitHub OAuth (Get these from GitHub Developer Settings)
GITHUB_ID=your-github-app-id-here
GITHUB_SECRET=your-github-app-secret-here
